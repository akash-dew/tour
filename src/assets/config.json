  "childLimit": 2,
  "actionCards" : {
    "studyPlan" : {
      "rules": [
        {
          "condition": "{{isPlanComplete}} === true",
          "msgKeys": ["sp_completed"]
        },
        {
          "condition": "{{planId}} !== undefined && {{planId}} !== '' && {{planId}} !== null && {{pauseDate}} !== undefined && {{pauseDate}} !== '' && {{pauseDate}} !== null",
          "msgKeys": ["sp_paused"]
        },
        {
          "condition": "{{planId}} !== undefined && {{planId}} !== '' && {{planId}} !== null && {{currenDate}} > {{planGoalEndDate}} && {{planFeasibility}} === false",
          "msgKeys": ["sp_deadline_missed"]
        },
        {
          "condition": "{{planId}} !== undefined && {{planId}} !== '' && {{planId}} !== null",
          "msgKeys": ["sp_in_progress"]
        },
        {
          "condition": "{{planId}} === undefined || {{planId}} === '' || {{planId}} === null",
          "msgKeys": ["no_sp"]
        }
      ]
    },
    "assignments" : {
      "rules": [
        {
          "condition": "{{activeAssignments}} === 0",
          "msgKeys": ["no_assign"]
        },
        {
          "condition": "{{activeAssignments}} === 1",
          "msgKeys": ["assign_lt_two"]
        },
        {
          "condition": "{{activeAssignments}} > 1 && {{activeAssignments}} <= 10",
          "msgKeys": ["assign_gt_one_lt_ten_active_gt_one"]
        },
        {
          "condition": "{{activeAssignments}} > 10",
          "msgKeys": ["assign_gt_ten"]
        }
      ],
      "messages" : {
        "assign_gt_ten" : "You have more than 10 Assignments Pending",
        "no_assign" : "{{activeAssignments}} assignments left",
        "assign_lt_two" : "{{activeAssignments}} assignment left",
        "assign_gt_one_lt_ten_active_gt_one" : "{{activeAssignments}} assignments left"
      }
    }
  }
